{% block slider %}

        <section id="feature_slider" class="">
            <!-- 
                Each slide is composed by <img> and .info
                - .info's position is customized with css in index.css
                - each <img> parallax effect is declared by the following params inside its class:
                
                example: class="asset left-472 sp600 t120 z3"
                left-472 means left: -472px from the center
                sp600 is speed transition
                t120 is top to 120px
                z3 is z-index to 3
                Note: Maintain this order of params
    
                For the backgrounds, you can combine from the bgs folder :D
            -->
            <article class="slide" id="tour" style="background-image:url({{ asset('theme1/color-splash.jpg')}}); display: none; background-position: 50% 0%; background-repeat: repeat no-repeat;">
                <img class="asset left-472 sp650 t210 z3" src="{{ asset('images/prefailles/visu_hebergement.jpg') }}" style="left: -558px; top: 210px; z-index: 3; display: inline;">
                <img class="asset left-365 sp600 t270 z4" src="{{ asset('theme1/iphone.png') }}" style="left: -485px; top: 270px; z-index: 4; display: inline;">


                {#<img class="asset left-350 sp450 t135 z2" src="{{ asset('theme1/desktop.png') }}"  style="left: -742px; top: 135px; z-index: 2; display: inline;">#}
                <img class="asset left-350 sp450 t135 z2" src="{{ asset('images/prefailles/visu_velo.jpg') }}"  style="left: -742px; top: 135px; z-index: 2; display: inline;">

